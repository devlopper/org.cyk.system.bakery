<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<!-- Do use PrettyFaces ONLY for navigation -->

	<navigation-rule>
		<from-view-id>*</from-view-id>
		
		<!-- Bakery Unit Manager -->
		<navigation-case>
			<from-outcome>bakeryUnitEditView</from-outcome>
			<to-view-id>/private/__role__/__bakeryunitmanager__/editonebakeryunit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>bakeryUnitListView</from-outcome>
			<to-view-id>/private/__role__/__bakeryunitmanager__/listonebakeryunit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>bakeryUnitProductionEditView</from-outcome>
			<to-view-id>/private/__role__/__bakeryunitmanager__/editonebakeryunitproduction.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>bakeryUnitProductionListView</from-outcome>
			<to-view-id>/private/__role__/__bakeryunitmanager__/listbakeryunitproduction.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>bakeryUnitProductionConsultView</from-outcome>
			<to-view-id>/private/__role__/__bakeryunitmanager__/consultonebakeryunitproduction.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>bakeryUnitDeliveryEmployeeProductionEditView</from-outcome>
			<to-view-id>/private/__role__/__bakeryunitmanager__/editonebakeryunitdeliveryemployeeproduction.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>bakeryUnitDeliveryEmployeeProductionListView</from-outcome>
			<to-view-id>/private/__role__/__bakeryunitmanager__/listbakeryunitdeliveryemployeeproduction.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>bakeryUnitDeliveryEmployeeProductionConsultView</from-outcome>
			<to-view-id>/private/__role__/__bakeryunitmanager__/consultonebakeryunitdeliveryemployeeproduction.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		
	</navigation-rule>

</faces-config>
